	<model name="Climate">
		<variables>
			<flow name="melting ice and\nshow">
				<eqn>Amount_of_Ice_and_Snow*melting_rate</eqn>
				<units>km3/Year</units>
			</flow>
			<stock name="Amount of Ice\nand Snow">
				<eqn>Amount_of_Ice_and_Snow_1980</eqn>
				<outflow>melting_ice_and_show</outflow>
				<units>km3</units>
			</stock>
			<aux name="melting rate">
				<eqn>melting_rate_surface_in_1980*(Surface_Temperature_Anomaly/Surface_Temperature_Anomaly_1980)</eqn>
				<units>1/year</units>
			</aux>
			<aux name="albedo">
				<eqn>albedo_1980*effect_of_ice_on_albedo</eqn>
				<units>1/year</units>
			</aux>
			<aux name="N2O concentration in atm PPM">
				<doc>Calibration data from:
Smith CJ, Forster PM, Berger S, Collins W, Hall B, Lunt D, Palmer MD, Watanabe M, Cain M, Harris G, Leach NJ, Ringer M, Zelinka M. 2021. Figure and data generation for Chapter 7 of the IPCC&apos;s Sixth Assessment Report, Working Group 1 (plus assorted other contributions). Version 1.0. https://doi.org/10.5281/zenodo.5211357</doc>
				<eqn>N2O_in_atmosphere/GtN2O_per_PPM</eqn>
				<units>ppm</units>
			</aux>
			<stock name="N2O in atmosphere">
				<eqn>N2O_in_atmosphere_1980</eqn>
				<inflow>N2O_emissions</inflow>
				<outflow>N2O_breakdown</outflow>
				<units>GtN2O</units>
			</stock>
			<aux name="GtN2O per PPM">
				<doc>Calibration Parameter: N2O concentration in PPM</doc>
				<eqn>2.91026088343104</eqn>
